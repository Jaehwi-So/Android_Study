    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".IntentSubActivity"
    android:orientation="vertical">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="kim"
        android:textSize="30dp"
        android:id="@+id/txt_name"></TextView>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="20"
        android:textSize="30dp"
        android:id="@+id/txt_age"></TextView>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="01011112222"
        android:textSize="30dp"
        android:id="@+id/txt_tel"></TextView>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2020-07-22"
        android:textSize="30dp"
        android:id="@+id/txt_birth"></TextView>
</LinearLayout>